    "build:production:server": "NODE_ENV=production webpack --config webpack.server.rails.config.js",
    "build:test": "npm run build:client && npm run build:server",
    "build:production": "npm run build:production:client && npm run build:production:server",
    "test": "istanbul cover _mocha -- --growl --reporter dot --opts ./mocha.opts",
